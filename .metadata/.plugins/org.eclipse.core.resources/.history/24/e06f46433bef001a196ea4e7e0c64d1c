package com.itperson.dto;

public class MAnswer {
	private int code;
	private String qConde;
	private String answer;
	private String correct;
	public int getCode() {
		return code;
	}
	public void setCode(int code) {
		this.code = code;
	}
	public String getqConde() {
		return qConde;
	}
	public void setqConde(String qConde) {
		this.qConde = qConde;
	}
	public String getAnswer() {
		return answer;
	}
	public void setAnswer(String answer) {
		this.answer = answer;
	}
	public String getCorrect() {
		return correct;
	}
	public void setCorrect(String correct) {
		this.correct = correct;
	}
	@Override
	public String toString() {
		return "MAnswer [code=" + code + ", qConde=" + qConde + ", answer=" + answer + ", correct=" + correct + "]";
	}
}
