package com.itperson.dao;

import java.util.ArrayList;

import com.itperson.dto.Field;


public interface FieldDao {
	public ArrayList<Field> courseList();
	public ArrayList<Field> categoryList();
	public ArrayList<Field> subCategoryList();
	public ArrayList<Field> partOfcategoryList(String coCode);
	public ArrayList<Field> partOfsubCategoryListByCategory(String caCode);
	public ArrayList<Field> PartOfsubCategoryListByCourse(String coCode);
	public Field serchCourseSet(String coCode);
	public Field serchCategorySet(String caCode);
	public Field serchSubCategorySet(String subCode);
	public Field subCategoryBelongs(String subCode);
	public Field categoryBelongs(String caCode);
}

